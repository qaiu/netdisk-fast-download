    "/v2/redirectUrl/{type}/{param}": {
      "get": {
        "tags": ["快捷下载"],
        "summary": "重定向下载URL（目录文件）",
        "description": "重定向到目录中指定文件的下载地址，param为Base64编码的参数",
        "parameters": [
          {
            "name": "type",
            "in": "path",
            "required": true,
            "description": "网盘类型标识",
            "schema": {
              "type": "string",
              "example": "pan"
            }
          },
          {
            "name": "param",
            "in": "path",
            "required": true,
            "description": "Base64编码的参数JSON",
            "schema": {
              "type": "string",
              "example": "eyJmaWxlSWQiOiIxMjM0NTYifQ=="
            }
          }
        ],
        "responses": {
          "302": {
            "description": "重定向到直链下载地址",
            "headers": {
              "Location": {
                "description": "重定向地址",
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "解析失败",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
