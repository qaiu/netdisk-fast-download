    "/v2/viewUrl/{type}/{param}": {
      "get": {
        "tags": ["预览接口"],
        "summary": "预览URL（目录预览）",
        "description": "预览目录中的文件，param为Base64编码的参数",
        "parameters": [
          {
            "name": "type",
            "in": "path",
            "required": true,
            "description": "网盘类型标识",
            "schema": {
              "type": "string",
              "example": "pan"
            }
          },
          {
            "name": "param",
            "in": "path",
            "required": true,
            "description": "Base64编码的参数JSON",
            "schema": {
              "type": "string",
              "example": "eyJmaWxlSWQiOiIxMjM0NTYifQ=="
            }
          }
        ],
        "responses": {
